[user]
	email = anthony.seure@gmail.com
	name = Anthony Seure
[color]
	ui = true
	branch = true
	diff = true
	interactive = true
	status = true
[alias]
	an = commit --amend --no-edit
	br = branch
	ci = commit
	co = checkout
	dc = diff --cached
	df = diff
	lg = log --color --graph --all --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	st = status
[push]
	default = simple
[core]
	excludesfile = ~/.gitignore_global
	ignorecase = false
