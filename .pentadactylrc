" Set Duck Duck Go as the default search engine
set defsearch=duckduckgo

" Set the colorscheme
colorscheme vimium

" Change GUI options
set guioptions=brnT

" Set the editor (Mac only for now)
set editor='/usr/local/bin/mvim -f +<line> +"sil! call cursor(0, <column>)" <file>a'

" Use Dvorak homerow keys instead of numbers as hint keys
set hintkeys=etuhfygp
